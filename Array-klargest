const a1=[1,2,3]
const k=2
var klargest=[]
function max(a,k){
let max=0;
let index=0;
for (let i=0; i<a1.length;i++){
    if (a[i]>max && !k.includes(a[i])){
        max=a[i];
        index=i;
    }
}
return max;
}
for (let j = 0; j<k; j++){
   klargest.push(max(a1,klargest ))
}
console.log(klargest);
